<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ComposeTemplate"
        tools:targetApi="31">
        <activity
            android:name=".TextFieldActivity"
            android:exported="false"
            android:label="@string/title_activity_text_field"
            android:theme="@style/Theme.ComposeTemplate" />
        <activity
            android:name=".SnackbarActivity"
            android:exported="false"
            android:label="@string/title_activity_snackbar"
            android:theme="@style/Theme.ComposeTemplate" />
        <activity
            android:name=".CheckboxActivity"
            android:exported="false"
            android:label="@string/title_activity_checkbox"
            android:theme="@style/Theme.ComposeTemplate" />
        <activity
            android:name=".ButtonActivity"
            android:exported="false"
            android:label="@string/title_activity_button"
            android:theme="@style/Theme.ComposeTemplate" />
        <activity
            android:name=".ShapeActivity"
            android:exported="false"
            android:label="@string/title_activity_shape"
            android:theme="@style/Theme.ComposeTemplate" />
        <activity
            android:name=".TextActivity"
            android:exported="false"
            android:label="@string/title_activity_text"
            android:theme="@style/Theme.ComposeTemplate" />
        <activity
            android:name=".BoxActivity"
            android:exported="false"
            android:label="@string/title_activity_box"
            android:theme="@style/Theme.ComposeTemplate" />
        <activity
            android:name=".RowAndColumn"
            android:exported="false"
            android:label="@string/title_activity_row"
            android:theme="@style/Theme.ComposeTemplate" />
        <activity
            android:name=".TestActivity"
            android:exported="false"
            android:label="@string/title_activity_test"
            android:theme="@style/Theme.ComposeTemplate" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.ComposeTemplate">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>